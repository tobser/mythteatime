	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/mythtv/plugins/ || $(MKDIR) $(INSTALL_ROOT)/mythtv/plugins/ 
	-$(INSTALL_PROGRAM) "$(TARGET)" "$(INSTALL_ROOT)/mythtv/plugins/$(TARGET)"

