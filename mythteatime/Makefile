main.o: main.cpp teatimeui.h \
		timerdata.h \
		data.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o main.cpp

teatimeui.o: teatimeui.cpp teatimeui.h \
		timerdata.h \
		edittimerui.h \
		data.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o teatimeui.o teatimeui.cpp

data.o: data.cpp data.h \
		teatimeui.h \
		timerdata.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o data.o data.cpp

timerdata.o: timerdata.cpp teatimeui.h \
		timerdata.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o timerdata.o timerdata.cpp

edittimerui.o: edittimerui.cpp edittimerui.h \
		teatimeui.h \
		timerdata.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o edittimerui.o edittimerui.cpp

